extends layout

block content
  section.main.clearfix
    h1.title-text header
    p.spacing All header styles are inside of the /modules/ _mod_header.scss directory.
    header.module-header
      div.top-box.clearfix
        div.company-logo
          img(src="/images/stein.svg", alt="company logo")
        section.top-navigation-mod
          ul
            li Home
            li English
            li
              img.globe-icon(src="/images/globe.svg", alt="globe icon")
      div.bottom-box.clearfix
        ul
          li Shop
          li Products
          li Support
          li Artist

  section.main.clearfix
    h1.title-text Footer
    p.spacing All footer styles are inside of the /modules/ _mod_footer.scss directory.
    footer.module-footer
      div.related-box.clearfix
        div.top-left
          ul
            li Shop
            li Products
            li Support
            li Artist
        div.social-box.clearfix
          div
            img(src="/images/facebook.svg", alt="facebook")
          div
            img(src="/images/googleplus.svg", alt="googleplus")
          div
            img(src="/images/soundcloud.svg", alt="soundcloud")
          div
            img(src="/images/youtube.svg", alt="youtube")
      div.copyrights-box.clearfix
        section.copy-left
          ul
            li Shop
            li Products
            li Support
            li Artist
        section.copy-right
          ul
            li Shop
            li Products
            li Support
            li Artist
        section
          h1.spacing Usage:
          pre.
            &lt;footer class=&quot;module-footer&quot;&gt;
              &lt;div class=&quot;related-box clearfix&quot;&gt;
                &lt;div class=&quot;top-left&quot;&gt;
                  &lt;ul&gt;
                    &lt;li&gt;Shop&lt;/li&gt;
