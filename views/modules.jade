                  &lt;/ul&gt;
                &lt;/div&gt;
                &lt;div class=&quot;social-box clearfix&quot;&gt;&lt;span&gt;&lt;img src=&quot;/images/facebook.svg&quot; alt=&quot;facebook&quot;&gt;&lt;/span&gt;&lt;span&gt;&lt;img src=&quot;/images/googleplus.svg&quot; alt=&quot;googleplus&quot;&gt;&lt;/span&gt;&lt;span&gt;&lt;img src=&quot;/images/soundcloud.svg&quot; alt=&quot;soundcloud&quot;&gt;&lt;/span&gt;&lt;span&gt;&lt;img src=&quot;/images/youtube.svg&quot; alt=&quot;youtube&quot;&gt;&lt;/span&gt;&lt;/div&gt;
              &lt;/div&gt;
              &lt;div class=&quot;copyrights-box clearfix&quot;&gt;
